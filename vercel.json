{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/backend/server.js"
    },
    {
      "source": "/(.*)",
      "destination": "/client/nissmart/$1"
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "outputDirectory": "client/nissmart/dist"
}
